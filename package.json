{
  "name": "@braw/rollup-plugin-icu-messages",
  "version": "0.0.0",
  "description": "Transform files containing ICU MessageFormat messages.",
  "author": {
    "name": "Alexander 'Brawaru' Sorokin",
    "url": "https://github.com/brawaru"
  },
  "repository": "github:Brawaru/rollup-plugin-icu-messages",
  "bugs": "https://github.com/Brawaru/rollup-plugin-icu-messages/issues",
  "homepage": "https://github.com/Brawaru/rollup-plugin-icu-messages#readme",
  "funding": "https://github.com/Brawaru/Brawaru/blob/main/SUPPORT.md",
  "keywords": [
    "i18n",
    "icu-messageformat",
    "rollup-plugin",
    "rollup",
    "plugin"
  ],
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./wrap-plugins": {
      "types": "./dist/wrap-plugins.d.ts",
      "import": "./dist/wrap-plugins.mjs",
      "require": "./dist/wrap-plugins.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build:raw": "del-cli dist && tsc --noEmit false",
    "build": "unbuild",
    "dev": "vitest dev",
    "prepack": "pnpm run build",
    "test": "vitest run",
    "lint": "eslint src",
    "bt": "pnpm run -s build && pnpm run -s test"
  },
  "engines": {
    "node": ">=16"
  },
  "devDependencies": {
    "@ltd/j-toml": "^1.38.0",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@rollup/plugin-json": "^6.0.0",
    "@types/glob": "^8.0.1",
    "@types/node": "^18.11.18",
    "del-cli": "^5.0.0",
    "eslint": "^8.31.0",
    "prettier": "^2.8.3",
    "prettier-plugin-jsdoc": "^0.4.2",
    "semantic-release": "^20.0.2",
    "typescript": "^4.9.4",
    "unbuild": "^1.1.1",
    "vitest": "^0.27.2"
  },
  "dependencies": {
    "@formatjs/cli-lib": "^5.1.12",
    "@formatjs/icu-messageformat-parser": "^2.1.14",
    "@rollup/pluginutils": "^5.0.2",
    "glob": "^8.1.0",
    "import-meta-resolve": "^2.2.1",
    "pathe": "^1.1.0"
  },
  "peerDependencies": {
    "rollup": "^3.11.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
